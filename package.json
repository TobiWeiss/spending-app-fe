{
  "name": "spending-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false",
    "e2e:mobile": "cypress open --browser=chrome --env target=mobile",
    "e2e:desktop": "cypress open --browser=chrome --env target=desktop",
    "e2e:ci:mobile": "npm start & wait-on http://localhost:4200 & cypress run --headless --browser chrome --env target=mobile",
    "e2e:ci:desktop": " npm start & wait-on http://localhost:4200 & cypress run --headless --browser chrome --env target=desktop",
    "cypress:open": "cypress open ",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@angular/service-worker": "^14.1.0",
    "cypress-real-events": "^1.7.1",
    "cypress-wait-until": "^1.7.2",
    "hammerjs": "^2.0.8",
    "primeicons": "^5.0.0",
    "primeng": "^14.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@cypress/schematic": "^2.0.3",
    "@types/jasmine": "~4.0.0",
    "@types/jest": "^28.1.8",
    "cy-mobile-commands": "^0.3.0",
    "cypress": "^10.6.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.7.2"
  }
}
